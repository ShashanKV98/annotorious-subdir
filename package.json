    "jsdom": "^22.1.0",
    "svelte": "^3.59.2",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "@annotorious/core": "*"
